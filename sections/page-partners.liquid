<div id="parnter_page">
<div class="container">
  <div class="text-left">
    <h3 class="page_header">{{ page.title }}</h3>
  </div>

  {% unless page.content == blank %}
  <div class="rte">
    {{ page.content }}
  </div>
  <ul class="partner_images">
    {% for block in section.blocks %}
    <li>
      <div class="img">
        <img src="{{block.settings.image | img_url : '100x' | format:'png'}}" data-img="{{block.settings.image | img_url : 'grande' | format:'png'}}" data-img-xs="{{block.settings.image | img_url : 'large' | format:'png'}}" class="img_load img_blur">
      </div>
    </li>
    {% endfor %}
  </ul>
</div>
</div>
{% endunless %}
{% schema %}
{
  "name": "Partners Page",
  "blocks": [
    {
      "name": "Partner image",
      "type": "image",
      "settings": [
        {
          "label": "Image",
          "id": "image",
          "type": "image_picker"
        }
      ]
    }
  ]
}
{% endschema %}

