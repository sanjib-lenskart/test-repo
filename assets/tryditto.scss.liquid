.bogo_new{
	.sunglass_new {
	    display: flex;
	    justify-content: space-between;
	    align-items: center;
	    .left{
	    	position: relative;
	    	text-align: center;
	    	& > div {
	    		padding: 5px 0;
	    		margin-bottom: 8px;
	    	}
	    }
	    .right{
	    	img{
		    	height: 141px;
		    	width: 143px;
	    	}
	    }
	    .title{
	    	font-size: 20px;
		    letter-spacing: 1px;
		    font-weight: 700;
		    color: #343434;
	    }
	    .product_img{
	    	img{
	    		max-width: 130px;
	    	}
	    }
	    .goldtext{
	    	font-size: 16px;
	    	color: #ee801c;
	    	letter-spacing: 1px;
	    }
	}
	.overlaytext{
		position: absolute;
	    top: -18%;
	    left: 0;
	    z-index: 99;
	    width: 100%;
	    height: 100%;
	    padding: 0;
	    margin-bottom: 0;
	    animation:fadeUp 0.3s;
	    margin: 0;
	    display: none;
		div{
			position: relative;
			background:#57ba90;
			font-size:20px;
			color:#fff;
			text-align:center;
			padding: 2em 0;
			line-height: 1.4;
    		padding-right: 3em;
    		font-weight: 300;
			.arroww{
				position: absolute;
				right: 30px;
				top:50%;
				transform:translateY(-50%);
				font-size:20px;
			}
		}
	}
	.sunglass_new{
		.left{
			&:hover{
	    		.overlaytext{
	    			display: block;
	    			opacity: 1;
	    			transform: translateY(0%);
	    		}
	    	}
		}
	}
}
