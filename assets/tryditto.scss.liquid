#shopify-section-product-lens-details {
	.col-sm-6{
		@media(min-width: 768px){
			overflow: hidden;	
		}
	}
	 img {
	 	@media(min-width: 768px){
		    max-width: 100%;
		    display: block;
		    margin: 0 auto;
		    float: right;
	 	}
	}
}
@keyframes fadeUp{
	0%{transform: translateY(30%); opacity: 0;}
	100%{transform: translateY(0%); opacity: 1;}
}


.collection-img-container{
  position: relative;    
  margin-bottom: 10px;
  clear: both;

  @media(min-width:992px){    
    padding: 40px 0;
  }

  @media(max-width:991px){
    border-top: 1px solid #ccc;
    padding-top: 20px;
    margin-top: 20px;
    border-bottom: 1px solid #ccc;    
    margin-left: 15px;
    margin-right: 15px;
  }

  a{
    @media(min-width:992px){
      position: relative;
      display: inline-block;
      width: 100%;
      border-top:1px solid #ccc;
      border-bottom: 1px solid #ccc;
      padding: 60px 0;

      img{float: right;}
    }
  }

  img{
    @media(max-width:991px){
      width:100%;
    }
  }

  .desktop_image{
    display:none;
    @media(min-width:768px){
      display:block;
    }
  }

  .mobile_image{
    display:none;
    @media(max-width:767px){
      display:block;
    }
  }

  .text-wrapper{
    @media(min-width:992px){
      position: absolute;   
      left: 5%;
      top: 50%;
      -webkit-transform: translateY(-50%);
      -ms-transform: translateY(-50%);
      transform: translateY(-50%);       
    }

    @media(min-width:1200px){
      left: 10%;
    }

    @media(min-width:1070px){
      max-width: 350px;
    }

    @media(max-width:991px){
      text-align: center;
      padding: 25px;
    }
    @media(min-width:992px) and (max-width:1070px){
      max-width: 250px;
    }

    h1 { 
      color: #414B57;
      text-transform: uppercase;
      font-weight: 500;    
      font-size: 16px;
      margin-bottom:20px;
      letter-spacing: 3px;

      @media(max-width:991px){
        font-size: 13px;
        margin-bottom:10px;
        letter-spacing: 1px;

        padding: 8px 12px;
        border: 1px solid;   
        display: inline-block;    
      }
    }

    .sub-header{
      color:#414B57;   
      font-weight: 500;    
      font-size: 22px;
      line-height:28px;
      letter-spacing: 1.2px;

      @media(max-width:991px){
        font-size: 16px;
        line-height: 18px;
        letter-spacing: 0.7px;
      }
    }

  }
}
#collection_image{
  position:relative;

  h1 { 
    color: #000000;
    text-transform: uppercase;
    font-weight: 500;    
    font-size: 22px;
    margin-bottom:20px;
    letter-spacing: 2px;

    @media(max-width:767px){
      font-size: 16px;
      margin-bottom:10px;
      letter-spacing: 2px;
      font-weight: 600;
    }
  }

  .sub-header{
    color: #000000;
    font-weight: 400;
    font-size: 20px;
    line-height: 32px;    

    @media(max-width:767px){
      font-size: 14px;
      line-height: 22px;
      letter-spacing: 0.2px;
    }

    .withgm{
      a{
        text-decoration: underline;
        color: #f9b23f;
        font-size: 22px;
        @media(max-width:767px){ font-size: 17px; }
      }
    }

  }

  .text-wrapper{   

    @media(min-width:768px){
      position: absolute;   
      left: 8%;
      top: 50%;
      -webkit-transform: translateY(-50%);
      -ms-transform: translateY(-50%);
      transform: translateY(-50%);
      max-width: 480px;
    }

    @media(max-width:767px){
      text-align: center;
      padding: 6px 25px;
    }
  }

}

