#head_try3d {
	cursor: pointer;
	text-transform:uppercase;
	.text-section & {
		text-transform:capitalize;
		a{
			color:#fff;
			text-decoration:none;
          text-transform: uppercase;
			&:active,&:focus{
				outline:none;
			}
		}
		&:hover a{
			color:#fff;
			text-decoration:none;
		}
	}
}
#collection_page{
	.try3d_enable {
	    position: absolute;
	    top: 20px;
	    z-index: 9;
	    transform: translateY(-50%);
	    -webkit-display: flex;
      	-moz-display: flex;
      	display: flex;
	    justify-content: space-between;
	    align-items: center;
	    background:transparent;
	    //font-size: 1.2em;
    	line-height: 1.5;
    	right: 0%;
    	@media(max-width:767px){
    		transform: translateY(0%);
          	justify-content:center;
          top: 0px;
          
    	}
    	@media(min-width: 1025px){
    		right: 5%;
          text-transform:uppercase;
    	}
    	@media (min-width: 768px) and (max-width: 1024px) and (orientation:portrait){
    		right: 0%;
    	}
    	@media (min-width: 768px) and (max-width: 1024px) and (orientation:landscape){
    		right: 0%; 
    	}
	    & > span {
	    	display: inline-block;
	    	margin: 0 8px;
	    }
		label{
	    	border-radius: 15px;
	    	background: #ccc;	
		    width: 70px;
		    height: 30px;
		    position: relative;
		    @media(max-width:767px){
    			height: 25px;
    		}
    		@media(max-width:320px){
    			width: 50px;
    			height: 20px;
    		}
		    .round_toggle {
			    display: inline-block;
			    width: 30px;
			    height: 30px;
			    background: #49a99b;
			    border-radius: 50%;
			    transition: all 0.15s linear;
			    position: absolute;
			    left: 0;
			    @media(max-width:767px){
			    	width: 25px;
    				height: 25px;
    			}
    			@media(max-width:320px){
    				width: 20px;
    				height: 20px;
    			}
			}
		}
		input[type="checkbox"]:checked ~ {
			& label {
				background:#a7d0b4; 
				.round_toggle{
					background:rgba(95, 181, 122, 0.7);
					left: calc(100% - 30px);
					@media(max-width:767px){
			    		left: calc(100% - 25px);
    				}
    				@media(max-width:320px){
    					left: calc(100% - 20px);	
    				}
				}
			}

		}
		.frame{
			@media(min-width:768px) and (max-width:1024px) and (orientation:portrait){
				display:none;
			}
		}
		.tryon_text{
			@media(max-width:767px){
				font-size: 15px;
				padding-top: 4px;
			}
			@media(max-width:320px){
				font-size: 13px;
			}
		}
      .frame{
       @media (min-width: 768px) and (max-width: 1324px){
      display:none;
    } 
      }
	}
	.collection{
		ul {
			li {
				.ditto_img{
					max-width: 100%;
					cursor:pointer;
					z-index: 2;
    				position: relative;
					display:none;
					@media(max-width:767px){
						margin-bottom:15px;
						margin-top:20px;
					}
				}
				.coll_thumbditto,.coll_ditto {
				    position: relative;
				    width: 350px !important;
    				height: 250px !important;
                  @media only screen and (max-width: 1080px) and (min-width: 768px){
                    width: auto !important;
                  }
    				@media(max-width:320px){
						width: 280px !important;
    					height: 280px !important;
    				}
				    max-width: 100%;
    				max-height: 100%;
    				background-color: transparent;
			    	background-image: url(https://static2.lenskart.com/media/desktop/img/loader-lk.gif);
				    background-repeat: no-repeat;
				    background-position: center;
				    background-size: 100%;
				    z-index: 2;
				    display: -webkit-box;
				    display: -moz-box;
				    display: -ms-flexbox;
				    display: -webkit-flex;
				    display: flex;
    				justify-content: center;
    				align-items: center;
    				display: none;
    				margin:0 auto;
    				@media(max-width: 767px){
					  margin:3em auto 2.5em;
    				}
    				img{ 
    					max-width: 100%;
    					display: none;
    					-webkit-object-fit: cover;
    					-moz-object-fit: cover;
    					-o-object-fit: cover;
    					-ms-object-fit: cover;
    					object-fit: cover;
    					-webkit-object-position: center;
    					-moz-object-position: center;
    					-o-object-position: center;
    					-ms-object-position: center;
    					object-position: center;
    				}
				}
			}
			& > li:first-child{
				.ditto_img{
					@media(max-width:767px){
						margin-top:0px;	
					}
				}
			}
		}
	}
	.collection_header{
		.filter_menu{
			.flex_view_xs{
				.try3d_enable {
					display:none;
					@media(max-width:767px){
						display:flex;
						position: relative;
					}
				}
			}
			.filter-tag{
				.try3d_enable {
					display:flex;
					@media(max-width:767px){
						display:none;
					}
				}
			}
		}
	}
}
