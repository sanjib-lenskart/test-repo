#account_page{
  .wrapper1, .wrapper2 { width: 100%; overflow-x: scroll; overflow-y: hidden; }
  .wrapper1 { height: 10px; }
  .wrapper2 {}
  .div1 { height: 10px; }
  .div2 { overflow: none; }

  @media(max-width:481px){
