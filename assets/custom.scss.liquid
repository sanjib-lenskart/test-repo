.loginpage_cus{
  background: #f6f6f6;
  min-height: 510px;
  .loginpage_cus_container{
    padding:20px;    
    max-width:350px;
    margin:auto;

    @media(max-width:767px){
      padding:20px 10px;
      max-width:300px;
      min-height:0px;
    }

    h3{
      @media(min-width:768px){
        text-align:center;
      }
    }

    #accordion {
      margin:20px auto;
      padding:0;
      width:100%;

      li {
        list-style:none;
        width:100%;
        position:relative;

        input[type='text'], input[type='email'],input[type='password']{
          border: .1rem solid #a6a6a6;
          border-top-color: #949494;
          border-radius: .3rem;
          box-shadow: 0 0.1rem 0 rgba(255,255,255,.5), 0 0.1rem 0 rgba(0,0,0,.07) inset;
          outline: 0;
          background:transparent;
        }

        input[type='submit']{
          box-shadow: 0 0.1rem 0 rgba(255,255,255,.4) inset;
          background: #49aa9c;
          background: -webkit-linear-gradient(top,#49aa9a,#49aa9c);
          background: linear-gradient(to bottom,#49aa9a,#49aa9c);
          border:none;
        }

        button{
          box-shadow: 0 0.1rem 0 rgba(255,255,255,.4) inset;
          background: #49aa9c;
          background: -webkit-linear-gradient(top,#49aa9a,#49aa9c);
          background: linear-gradient(to bottom,#49aa9a,#49aa9c);
          border:none;
          margin-top: 5px;
          margin-bottom: 15px; 
          height: 40px;
        }
      }  
      .errors{
        li{
          color:red;
          margin-bottom: 8px;
        }
      }
    }
    #accordion  li .label {
      padding: 12px 1px 10px 35px;
      display:block;
      font-size:14px;
      line-height:20px;
      color:#000;
      background-color:#f3f3f3;
      border: 0.5px #ddd solid;
      cursor: pointer;
      font-weight:600;

      span{
        font-size:13px;
        font-weight:normal;
      }
    }    
    /* Show the radio buttons */
    #accordion .label + input[type='radio'] {
      position: absolute;
      top: 12px;
      left: 5px;
      width:23px;
      height:23px;
      border:1px solid #ccc;
      -webkit-appearance: none;
      -moz-appearance: none;
      appearance: none;
      display: inline-block;
      background-color: #fff;    
      border-radius: 50%;
      cursor: pointer;     
      margin-right: 7px;
      outline: none;
    }    
    /* hide content by default */
    #accordion .content {
      display:block;
      background-color:#fff;
      color:#000;
      border: 1px #ddd solid;
      padding: 0 15px;
    }
    /* Show content boxes when the radio buttons are checked */
    #accordion .label + input[type='radio']:checked + .content {
      //display:block;
    }

    .form-radio:checked::before
    {
      position: absolute;
      color: #49aa9c;
      font-size:18px;
      content: '\25CF';
      top: 50%;
      left: 50%;
      -webkit-transform: translate(-50%, -50%);
      -moz-transform: translate(-50%, -50%);
      -ms-transform: translate(-50%, -50%);
      -o-transform: translate(-50%, -50%);
      transform: translate(-50%, -50%);
    }
    .form-radio:hover
    {
      background-color: #fff;
    }
    .form-radio:checked
    {
      background-color: #fff;
      box-shadow: 0 0 0.3rem 0.2rem rgb(197, 227, 223);
    } 

    #login_page, #register_page{ 
      .form_vertical{
        padding: 20px 0;
        @media(max-width:767px){
          padding: 10px 0;
        }  

        input{
          margin-top: 5px;
          margin-bottom: 15px; 
        }
      }
    }
  }

}

