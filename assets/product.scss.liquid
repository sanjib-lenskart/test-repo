    min-height: 300px;
    margin: 0 auto;
    box-shadow: 0 10px 50px 0 #ccc;
    margin-bottom: 2rem;

    @media(max-width:480px){
      width: 290px;
    }

    input, button, textarea {
      appearance: none;
      border: none;
      font-size: inherit;
      background: #fff;
      border-radius: 3px;
      padding: 1rem;
      width: 100%;
      border: 1px solid #ccc;
    }

    select {
      appearance: none;
      border: none;
      font-size: inherit;      
      border-radius: 3px;      
      width: 100%;
      border: 1px solid #ccc;
    }

    input,select,textarea {
      margin-bottom: 1rem;
    }

    input:focus {
      outline: 1px solid #F18260;
    }

    button {
      color: #fff;
      cursor: pointer;
      background-color: #252431;
    }

    button:hover {
      background-color: #F18260;
    }

