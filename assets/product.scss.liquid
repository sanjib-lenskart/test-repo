/*================ Font variable ================*/
$abt-frame-content-color: #414B56;
$pro_title_font: {{settings.pro_title}};
$btn_color : {{settings.btn_color}};
$btn_hover_color : {{settings.btn_hover_color}};
$btn_disabled_color:  {{settings.btn_disabled_color}};
$btn_font_color : {{settings.btn_font_color}};
$btn_font_hover_color : {{settings.btn_font_hover_color}};

@mixin flexview-start{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
@mixin custom_radio_round{
  position: absolute;
  background: #d1d6d9;
  width: 24px;
  height: 24px;
  left: 0px;
  box-sizing: border-box;
  border: 0;
  box-shadow: none;
  top: 2px;
  transition: background 0.1s ease-in-out;
  border-radius: 50%;
  overflow: hidden;
}
@mixin custom_checkmark{
  position: absolute;
  left: 8px;
  top: 14px;
  width: 2.5px;
  height: 8px;
  border: solid #fff;
  border-width: 0 1.3px 1.3px 0;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg) translate(-50%, -50%);
  transform-origin: center;
  transition: border-color 0.1s ease-in-out;
}
@mixin card-round{
  width: 100%;
  display: block;
  padding: 18px;
  border: 1px solid #e1e5e6;
  margin-bottom: 0;
  border-radius: 4px;
  @media(max-width: 767px){
    padding: 2px;
  }
}
@mixin arrow-left{
  display: block;
  position: absolute;
  width: 15px;
  height: 15px;
  background-color: #fff;
  left: -13px;
  top: 50%;
  -webkit-transform: rotate(45deg) translateY(-50%);
  -ms-transform: rotate(45deg) translateY(-50%);
  transform: rotate(45deg) translateY(-50%);
  border-radius: 4px;
  border-left: 1px solid #d1d6d9;
  border-bottom: 1px solid #d1d6d9;
  overflow: hidden;
}
@mixin sub_head{
  font-size: 16px;
  line-height: 24px;
  color: #676F74;
  font-weight: 400;
    @media(max-width:481px){
      font-size:14px;
      }  
    
}
@mixin btn-black{
  text-align: center !important;
  font-size: 14px;
  line-height: 1;
  padding: 10px 0;
  font-weight: 900;
  border-radius: 0;
  background: $btn_color;
  border: 2px solid $btn_color;
  color: $btn_font_color;
  letter-spacing: 1.5px;
  transition: 0.3s;
}
@mixin align-center-via-margin{
  margin: 0 auto;
  display: block;
}
@mixin flexview-sb{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
@mixin list-item {
  ul {
    padding-left: 18px;
    @media(max-width: 767px){
      padding-left: 15px;
    }
    li {
      font-weight: normal;
      font-size: 16px;
      color: $abt-frame-content-color;
      line-height: 2;
      list-style: disc; 
      font-weight: 400;
      color: #343434;
      @media(max-width: 767px){
        font-size: 14px;
        font-weight: 400;
      }
    }
  }
}

