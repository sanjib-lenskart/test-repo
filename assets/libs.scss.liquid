/*================ Tables ================*/
.table{
  overflow: auto;
  &.table-center{
    tr,th,td{
      text-align: center;
    }

  }
  &.row-color{
    tbody,tfoot{
      tr{
        &:nth-child(even) {
          background-color: $row_background_color;
        }
        &:hover{
          background-color: $row_hover_color;
        }
      }
    }
  }
  &.full-color{
    thead{
      tr{
        background-color:$thead_background; 

        th{
          color:$th_font_color;
        }
      }
    }
    tbody, tfoot{
      tr{
        &:nth-child(even) {
          background-color: $row_background_color;
        }
        &:hover{
          background-color: $row_hover_color;
        }
      }
    }
  }
  table{
    width:100%;
    thead{
      tr{
        border: 1px solid $table_border_color;
        th{
          padding: 8px;
          display: table-cell;
          text-align: left;
          vertical-align: middle;
          border-radius: 2px;
          font-weight: normal;
          border-color: #fff;
          border-right: 1px solid $table_border_color;
          min-width: 100px;
          @media(max-width:480px){
  min-width: 62px;
}
        }
      }
    }
    tbody,tfoot{
      tr{
        border-top:none; 
        border: 1px solid $table_border_color;

        td{
          border-right:1px solid $table_border_color; 
          padding: 8px;
          min-width: 100px;
@media(max-width:480px){
  min-width: 62px;
}
        }
      }
    }
  }
}
/*================ Form ================*/
input{ 
  -webkit-appearance: none;
  -moz-appearance: none;
  -o-appearance:none;
  -ms-appearance:none;
  appearance: none;
  -webkit-border-radius: 0;
  border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  width: 100%;
  font-size:14px;
  max-width: 320px;
  color: #000000;
  padding: 10px 5px;
  background: #f1f1f1;
  height:40px;
  outline:none;
  border:1px solid #ddd;
  &:focus{
    outline-style: none;
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 4px rgba(0,0,0,0.2);
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 4px rgba(0,0,0,0.2);
    -moz-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 4px rgba(0,0,0,0.2);
    -o-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 4px rgba(0,0,0,0.2);
    -ms-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 4px rgba(0,0,0,0.2);
    border-color: #bbb;
    -webkit-transition: border linear 0.2s, box-shadow linear 0.2s;
    -moz-transition: border linear 0.2s, box-shadow linear 0.2s;
    -o-transition: border linear 0.2s, box-shadow linear 0.2s;
    transition: border linear 0.2s, box-shadow linear 0.2s;
  }
}
input[type='text'],input[type='email'],input[type='password']{
  text-indent:5px;
}
input[type='checkbox']{
  width:auto;
  height:auto;
  -webkit-appearance: checkbox;
  -moz-appearance: checkbox;
  -o-appearance:checkbox;
  -ms-appearance:checkbox;
  appearance: checkbox;
  margin:0 0 0 0;
}
input[type='radio']{
  width:auto;
  height:auto;
  -webkit-appearance: radio;
  -moz-appearance: radio;
  -o-appearance:radio;
  -ms-appearance:radio;
  appearance: radio;
  margin:0 0 0 0;
}
select{
  -webkit-appearance: none;
  -moz-appearance: none;
  -o-appearance:none;
  -ms-appearance:none;
  appearance: none;
  background:transparent;
  border:1px solid #ddd;
  -webkit-border-radius: 0;
  border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  min-width:150px;
  color: #000000;
  padding:0 25px 0 10px;
  height:40px;
  outline:none;
  background:url({{'select.png' | asset_url}}) no-repeat;
  background-position: calc(100% - 8px) center;
  background-size: 15px;
}
#contact_page{
.form_vertical{
  .half{
    display:inline-block;
    width:48%;
  }
  label{
    display:block;
    margin-bottom:5px;
    &.inline-block{
      display:inline-block;
      margin-bottom:15px;
      vertical-align:top;
    }
    
    @media(max-width:481px){
      text-align:left;
    }
  }
  input,select,textarea{
    margin-bottom:15px;
    @media(max-width:481px){
      max-width:100%!important;
    }
  }
  input[type='radio'],input[type='checkbox']{
    margin-bottom:0px;
  }
  textarea{
    max-height:85px;
    width:100%;
    max-width:320px;
    outline:none;
    background: #f1f1f1;
    border: 1px solid #ddd;
  }
  .form-success{
    color: #56ad6a;
    background-color: #ecfef0;
    border-color: #56ad6a;
    border: 1px solid;
    padding: 10px;
    margin-bottom:20px;
  }
  .form-error, .errors{
    color: red;
    background-color: #ecfef0;
    border-color: #56ad6a;
    border: 1px solid;
    padding: 10px;
    margin-bottom:20px;
  }
  
  .btn{
   width:100%; 
  }
}
}
/*================ Button ================*/
button ,.btn{
  font-family: 'Montserrat', sans-serif;
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
  -webkit-border-radius: 0;
  border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  text-transform: uppercase;
  text-align: center;
  cursor: pointer;
  display: inline-block;
  width: auto;
  outline: 0;
  border: 0;
  padding:0;
  -webkit-transition: all 0.2s ease;
  -moz-transition: all 0.2s ease;
  -ms-transition: all 0.2s ease;
  -o-transition: all 0.2s ease;
  transition: all 0.2s ease;
  line-height: 1;
  margin: 0;
  border:1px solid;
  font-weight: normal;
  @media(max-width:551px){
    font-size:12px;
    padding:8px 8px;
  }
}
button{
  background:transparent;
}
.btn{
  background:$btn_color;

  color : $btn_font_color;
  border-color:$btn_color;
  padding:10px 12px;
  &.disabled{
    background:$btn_disabled_color;
    border-color:$btn_disabled_color;
    &:hover{
      background:$btn_disabled_color;
      color : $btn_font_color;
    }
  }
  &:hover{
    background:$btn_hover_color;
    color:$btn_font_hover_color;
  }
  &.reverse{
    background:transparent;
    color:#000;
    &:hover{
      background:#000;
      color:#fff;
    }
  }
}

.btn-large{
  width: 48%;
  display:block;
  padding: 6px 20px;
  text-align: center;
  margin: 22px auto 0px;
  border: 1px solid rgb(65, 64, 66);
  color: rgb(65, 64, 66);
  text-transform: uppercase;
  font-size: 18px;
  transition:.3s all;
}

/*================ Simply product common ================*/
.product_main{
  position:relative;
  &.disabled{
    opacity:.5;
  }
  .quick_view{
    @media(max-width:1024px){
      display:block !important;
    }
    @media(min-width:1025px){
      display:none;
    }
    position:absolute;
    z-index:3;
    display:block;
    width:100%;
    text-align:center;
    background:rgba(255, 255, 255, 0.7);
    cursor:pointer;
    top:50%;
    padding:5px 0;
    @include transform(translateY(-50%));
  }
  &.sold_out{
    .product_img{
      opacity:.5 !important;
    }
  }
  .product_link{
    display:block;
    position:relative;
    overflow:hidden;
    .product_img{
      position:relative;
      z-index:2;
    }
    .alter_img{
      display:none;
      position: absolute;
      top: 12%;
      left: 0;
      right: 0;
      bottom: 0;
      z-index:1;
    }
  }
  .new_product{
    position: absolute;
    right: 0;
    top: 0;
    z-index: 3;
    @media(max-width:767px){
      top:10px;
    }
  }
  .image{
    display:table;
    width:100%;
    position:relative;
    .img{
      display:table-cell;
      vertical-align:middle;

      .sold_out{
        border:1px solid $sold_color;
        color:$sold_color;
      }
      .on_sale{
        color:$sale_color;
        border:1px solid $sale_color;
      }


      .on_sale,.sold_out{
        position:absolute;
        left:10px;
        top:10px;
        border-radius:100%;
        width:50px;
        height:50px;
        text-align:center;
        z-index:2;
        p{
          margin:0;
          font-size:14px;
          line-height:1;
          position:absolute;
          left:50%;
          top:50%;
          @include transform(translate(-50%,-50%));
        }
      }
    }
  }
  .text{
    .thin_title{
      margin-bottom:5px;
    }
    .product_link{
      transition: .3s all; 
      display:block;
      .product_title{
        margin-bottom:5px;
        font-weight: 500;
        font-size:20px;
        letter-spacing: 0.7px;
        -webkit-font-smoothing: antialiased;
      }
    }

    text-align:center;
    margin:8px 0;
    @media(max-width: 767px){
      margin:6px 0px 0px;
    }
  }
}
.product_price{
  display: block;
  color: #76BAAC;
  font-size: 20px;
  font-weight: 700;
  .note{
    font-size: 13px;
    display: inline-block;
    line-height: 1.1;
    color: #BFBFBF;
    font-weight: normal;
  }
  .compare{
    text-decoration:line-through;
    color:#F0B9AA;
    margin-left:3px;
  }
}
.product_price.product_eyeglasses {
  margin-top: -13px;
  display: block;
  vertical-align: top;
  padding: 0 19px 0 0;
  line-height: 38px !important;
  margin-bottom:0 !important;
  @media(max-width:767px){
    margin-top: 0px;
  }
}


/*================ Pagination ================*/
.pagination{
  padding:25px 0 15px;
  text-align:center;
  transition: .3s all;
  .page{
    padding:8px;
    &.current{
      color:#a26b25;
    }
  }
}

/*================ section comman ================*/
.shopify-section{
  padding:$section_space 0;
  &.no_padding{
    padding:0;
  }
  &.no_bottom_padding{
    padding-bottom:0;
  }
  &.no_top_padding{
    padding-top:0;
  }
  @media(max-width:767px){
    padding:10px 0;
  }
  h3.title{
    margin-top:-.6em;
    margin-bottom:20px;    
  }
}

/*================ Header ================*/
.mm-page{
  &.active{
    @include transform(translateX(-400px));
    @media(max-width:551px){
      @include transform(translateX(-300px));
    }
  }
}
.table_view_xs{
  >div[class^="col"] {
    padding-left:0;
    position:static;
    padding-right:0;
  }
  display:table;
  width:100%;
  >div[class^="col"] {
    float:none;
    padding-left:0;
    padding-right:0;
    display:table-cell;
    vertical-align:middle;
  }
}
.table_view{
  >div[class^="col"] {
    padding-left:0;
    position:static;
    padding-right:0;
  }
  @media(min-width:768px){
    display:table;
    width:100%;
    >div[class^="col"] {
      float:none;
      padding-left:0;
      padding-right:0;
      display:table-cell;
      vertical-align:middle;
    }
  }
}

.inline_view{
  >div[class^="col"] {
    padding-left:0;
    position:static;
    padding-right:0;
  }
  @media(min-width:768px){
    display:block;
    width:100%;
    >div[class^="col"] {
      float:none;
      padding-left:0;
      padding-right:0;
      display:inline-block;
      vertical-align:middle;
      margin-right:-4px;
    }
  }
}

/*================ Topbar ================*/

#top_bar{
  p{
    line-height:40px;
    margin-bottom:0;
    padding-right:25px;
    font-size:14px;
    color:#777;
  }
  .menu{
    ul{
      text-align:right;
      li{
        display:inline-block;
        &:last-of-type{
          a{
            margin-right:0;
          }
        }
        a{
          line-height: 40px;
          margin-right: 10px;
          height: 40px;
          font-size:14px;
          display: block;
          transition:.3s all;
          text-transform:uppercase;
        }
      }
    }
  }
}

