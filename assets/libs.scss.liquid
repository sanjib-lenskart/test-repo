/*================ Order page ================*/
$active_line : #80cbc4;
$cancelled_line : #e36c66;
#order_page{
  .order_status{
    border-top:1px solid #ddd;
    border-bottom:1px solid #ddd;
    padding: 30px 0px;
    margin-bottom: 20px;
    .order_processing{
      .processing_text{
        line-height: 1;
        position: relative;
        top: -15px;
        z-index: 10;
        background: #80cbc4;
        color: #fff;
        display: inline-block;
        padding: 8px 12px;

      }
      &.active{
        &:before{
          background:$active_line;
          margin-top: -16.5px;
        }
        &:after{
          margin-top: -16.5px;
        }
      }
      &.after_active{
        &:after{
          background:$cancelled_line;
        }
      }
    }
    .order_Placed{
      &:before{
        width:0% !important;
      }
      &:after{
        background: #7fcbc4 !important;
      }
    }
    .order_packed,.order_dispatched{
      &.after_active{
        &:after{
          background:$active_line;
        }
      }
    }
    .order_completed{
      &:after{
        width:0% !important;
      }
    }
    .order_icon{
      text-align: center;
      position: relative;
      display: table-cell;
      vertical-align: middle;
      z-index: 10;
      overflow: hidden;
      &.cancelled{
        &:before{
          background:$cancelled_line;
        }
      }
      &.active{
        &:before{
          background:$active_line;
        }
      }
      .img{
        position: relative;
        z-index: 100000000;
        margin-bottom: 10px;
        img{
          max-width: 60px;
          @media(max-width:767px){
            max-width: 30px;
          }
        }
      }
      span{
        @media(max-width:767px){
          font-size:10px;
        }
      }
      &:after{
        content: '';
        position: absolute;
        width: 100%;
        height: 5px;
        background: #ecf0f1;
        top: 50%;
        margin-top: -15.5px;
        left: 50%;
        z-index: 1;
      }
      &:before{
        content: '';
        position: absolute;
        width: 100%;
        height: 5px;
        background: #ecf0f1;
        top: 50%;
        margin-top: -15.5px;
        left: -50%;
        overflow: hidden;
      }
    }
  }
  .table{
    margin-bottom:25px;
  }
  .return_btn{
    font-size: 16px;
    margin: 0;
    padding: 20px 0 0;
  }
  h1,h2{
    font-weight:normal;
  }
  .order_table{
    padding-bottom:25px;
  }
  .summary{
    padding-top:25px;
  }
}
