//toggle btn
.flat-toggle {
  @media(max-width:767px){
    padding-bottom:10px;
  }
  @media(min-width:768px){
    margin-bottom:-25px;
    padding:10px 0;
  }
}
.flat-toggle .toggle_background {
  width: 32px;
  border-radius: 4px;
  height: 10px;
  background-color: #DDD;
  position: relative;
  -moz-user-select: none;
  -khtml-user-select: none;
  -webkit-user-select: none;
  -o-user-select: none;
  user-select: none;
  position:relative;
  margin: 6px 10px;
}
.flat-toggle .toggle_btn {
  border-radius: 100%;
  background-color: #ADADAD;
  position: absolute;
  left: 0;
  top: -3px;
  width: 15px;
  height: 15px;

  -webkit-transform: translate(0);
  -ms-transform: translate(0);
  transform: translateX(0);
  transition: all 0.2s ease-in-out;
  -webkit-transition: all 0.2s ease-in-out;
  -moz-transition: all 0.2s ease-in-out;
}

.flat-toggle > span {
  position: relative;
  top: -2px;
  color: #666;
  white-space: nowrap;
  display: inline-block;
  vertical-align:top;
  cursor: pointer;
  &.color{
    color: #00A3D9;
  }
}

.flat-toggle:hover {
  cursor: pointer;
}

.flat-toggle:hover span {

  cursor: pointer;
}

.flat-toggle.on {
  transform: translate3d(0,0,0);
}

.flat-toggle.on:hover span {
  cursor: pointer;
}

.flat-toggle.on {
  .toggle_background{
    background-color: #8CE2FF;
  }
  .toggle_btn{
    background-color: #00A3D9;
    @include transform(translate(17px));
  }

}
