header.main_header{
  position:relative;
  {% if settings.header_transparency %}
  @include background-opacity($header_background, .9);
  {% else %}
  @include background-opacity($header_background, 1);
  {% endif %}

  -webkit-box-shadow: 0px 0px 5px 1px #999;
  -moz-box-shadow: 0px 0px 5px 1px #999;
  -o-box-shadow: 0px 0px 5px 1px #999;
  -ms-box-shadow: 0px 0px 5px 1px #999;
  box-shadow: 0px 0px 5px 1px #999;
  &.fixed{
    position:fixed;
    top:-100%;
    left:0;
    z-index:101;
    width:100%;
    margin:0;
    -webkit-transition: all 900ms ease-in-out;
    -mos-transition: all 900ms ease-in-out;
    -ms-transition: all 900ms ease-in-out;
    -o-transition: all 900ms ease-in-out;
    transition: all 900ms ease-in-out;
    &.active{
      top:0;
    }
  }
  a{
    transition:.3s all;
  }
  .site_logo{
    padding:8px 0;
    a{
      display:block;
      img{
        width:100%;
        height:auto;
      }
    }
    h6{
      text-transform:uppercase;
    }
  }
  .main_menu{
    @media(min-width:768px){
      padding-left:15px;
      padding-right:15px;
    }
    .main_nav{
      width:100%;
      @media(max-width:1024px){
        display:none;
      }
      &.hide_menu{
        visibility:hidden;
        position:absolute;
        z-index:-10;
      }
    }
    .ul{
      width:100%;
      .li{
        float:left;
        position: relative;
        &:last-of-type{
          >a{
            margin-right:0px;  
          }
        }
        a{
          line-height:1;
          display:block;
          color : $menu_link_color;
          transition:.5s all;
          padding: 3px 15px;
          font-size: 14px;
          &:hover{
            color:$menu_link_hover_color;
          }
          span{
            line-height:1;
          }
          .fa{
            margin-left:4px;
          }
        }
        >a{
          margin-right:4px;
          padding:15px;
          display:inline;
          &:hover{
            padding-bottom:100%;
          }
        }

        .child_menu{
          position: absolute;
          display:none;
          background: #fff;
          margin-top: 14px;
          left: 0;
          z-index: 100;
          .menu_wrapper{
            {% if settings.header_transparency %}
            @include background-opacity($header_background, .9);
            {% else %}
            @include background-opacity($header_background, 1);
            {% endif %}
            border: 1px solid #ddd;
          }
          &.mega{
            left: 0;
            width:550px;
            .menu_wrapper{
              border:0;
              padding:13px 0 13px 0;
            }
            .child_ul{
              float:left;
              padding:0 15px;
              width:20%;
              .child_li{
                width:auto;
                a{
                  font-size:14px;
                }
              }
            }
            .menu_product{
              padding:0 15px;
              width:80%;
              float:left;
              margin-left:-15px;
              margin-right:-15px;
              .menu_product_li{
                width:20%;
                float:left;
                padding:0 15px;
              }
              .product_main{
                padding:10px 0;
                .product_price{
                  padding:5px 0;
                }
                .brand{
                  display:none;
                }
              }
            }
          }
          &.no_mega{
            .child_li{
              position:relative;
            }
          }
          .child_ul{
            display:inline-block;
            padding:10px 0;
            .child_li{
              min-width:150px;
              >a{
                padding:3px 15px;
                font-size:14px;
              }
              .fa-caret-right{
                margin-left:4px;
              }
            }
            .link_data{
              position:absolute;
              z-index:10;
              left:100%;
              top:0px;
              display:none;
              min-width:150px;
              border:1px solid #ddd;
            }
            .third_menu{
              background:#fff;
              padding:10px 20px;
              li{
                a{
                  padding:6px 0;
                  font-size:13px;
                }
              }
            }

          }
        }
      }
    }
  }

  .mobile_search{
    @media(min-width:768px){
      display:none !important;
    }
    position:absolute;
    left:0;
    right:0;
    top:-5px;
    bottom:-5px;
    .close{
      position: absolute;
      right: 0;
      width:20px;
      text-align:center;
      top: 50%;
      color: #fff;
      right: 15px;
      @include transform(translateY(-50%));
      font-size: 20px;
      cursor: pointer;
    }
    .search_form{
      height:100%;
    }
    .search{
      max-width:100%;
      height:100%;
      input{
        height:100%;
        width:100%;
        max-width:100%;
        background:#000;
        color:#eee;
      }
      button{
        display:none;
      }
    }
  }
  .right_section{
    text-align:right;
    @media(max-width:767px){
      display:none;
    }
    .mega_search{
      position:absolute;
      left:0;
      top:0;
      bottom:0;
      right:0;
      display:none;
      z-index:100;
      .close{
        position:absolute;
        right:25px;
        @include transform(translateY(-50%));
        top:50%;
        cursor:pointer;
        font-size:30px;

      }
      .search_form{
        height:100%;
        form,input{
          height:100%;
        }
        form{
          max-width:initial;
        }
        input{
          padding-left:40px;
          margin:0;
        }
        button{
          right: initial;
          left: 5px;
          top: 50%;
          font-size: 25px;
          @include transform(translateY(-50%));
          width: 35px;
          padding: 0;
        }
      }
    }
    .col{
      display:inline-block;
      vertical-align:middle;
      padding:0 5px;
      &.cart_wrapper{

        .cart{
          font-size:18px;
        }
      }
    }
  }
