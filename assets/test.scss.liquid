    @media(max-width:767px){
      height:30px;
      width:30px;
    }
  }
  input[type=number]::-webkit-inner-spin-button, 
  input[type=number]::-webkit-outer-spin-button { 
    -webkit-appearance: none; 
    margin: 0; 
  }
  .min,.plus{
    width: 30px;
    position: absolute;
    top: 0;
    bottom: 0;
    border: 1px solid #ddd;
    background: #eee;
    font-size: 24px;
    cursor: pointer;
    line-height: 38px;
    text-align:center;
    @media(max-width:767px){
      line-height:30px;
    }
    &::selection{
      background: transparent;
    }

  }
  .min{
    left: 0%;
    border-right: none;
  }
  .plus{
    right:0;
    border-left: none;
  }
}
