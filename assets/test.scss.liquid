        opacity:.7;
        transition:.5s all;
        padding:0;
        &:hover{
          opacity:1;
        }
        a{
          width: 21px;
          height: 21px;
          box-sizing: border-box;
          border-radius: 100%;
          text-align: left;
          text-indent: -9999px;
          display: inline-block;
          transition: .5s all;
          box-shadow: 4px 4px 4px #ddd;
          &:hover{
            transition: box-shadow .3s;
            box-shadow: 0 0 0 1px #999;
            opacity: 1;
          }
        }
        &.active-filter{
          opacity: 1;
          a{
            transition: box-shadow .3s;
            box-shadow: 0 0 0 2px #000;
          }
        }
      }
    }
  }
