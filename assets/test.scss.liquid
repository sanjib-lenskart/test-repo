/*================ Simply product common ================*/
.product_main{
  position:relative;
  &.disabled{
    opacity:.5;
  }
  &:hover{
    .quick_view{
      /*display: block;*/
    } 
  }
  .quick_view{
    @media(max-width:767px){
      display:none !important;
    }
    @media(max-width: 1024px) and (min-width: 768px){
      display:none;
    }
    @media(min-width:1025px){
      display:none;
    }
    position:absolute;
    z-index:3;
    display:block;
    width:100%;
    text-align:center;
    /*     background:#3f3f3f; */
    background: rgba(255,255,255,.7);
    cursor:pointer;
    /*     top:50%; */
    bottom : 0;
    padding:5px 0;
    /*     @include transform(translateY(-50%)); */
    color:#3f3f3f;
  }
  &.sold_out{
    .product_img{
      opacity:.5 !important;
    }
  }
  .product_link{
    display:block;
    position:relative;
    overflow:hidden;
    .product_img{
      position:relative;
      z-index:2;
    }
    .alter_img{
      display:none;
      position: absolute;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      z-index:1;
    }
  }
  .on_sale,.sold_out{
    position: absolute;
    left: -5px; top: -5px;
    z-index: 2;
    overflow: hidden;
    width: 75px; height: 75px;
    text-align: right;
    span {
      font-size: 10px;
      font-weight: bold;
      color: #FFF;
      text-transform: uppercase;
      text-align: center;
      line-height: 20px;
      transform: rotate(-45deg);
      -webkit-transform: rotate(-45deg);
      width: 100px;
      display: block;
      background: #79A70A;
      background: linear-gradient(#F70505 0%, #8F0808 100%);
      box-shadow: 0 3px 10px -5px rgba(0, 0, 0, 1);
      position: absolute;
      top: 19px; left: -21px;
      &:before{
        content: "";
        position: absolute; left: 0px; top: 100%;
        z-index: -1;
        border-left: 3px solid #8F0808;
        border-right: 3px solid transparent;
        border-bottom: 3px solid transparent;
        border-top: 3px solid #8F0808;
      }
      &:after{
        content: "";
        position: absolute; right: 0px; top: 100%;
        z-index: -1;
        border-left: 3px solid transparent;
        border-right: 3px solid #8F0808;
        border-bottom: 3px solid transparent;
        border-top: 3px solid #8F0808;
      }
    }
    p{
      margin:0;
      font-size:14px;
      line-height:18px;
    }
  }
  .image{
    display:table;
    width:100%;
    position:relative;
    .img{
      display:table-cell;
      vertical-align:middle;

    }
  }
  .text{
    .product_link{
      transition: .3s all;
      display:block;
      .product_title{
        margin-bottom:10px;
      }
    }

    text-align:center;
    margin:8px 0;
  }
}
