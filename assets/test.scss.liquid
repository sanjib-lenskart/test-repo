}

.product_main .image .img {
  display:block;
}
.product_link.product_img{
  width:auto;
  width : 275px;
  height:275px;
  max-width :100%;
  max-height:100%;
  margin:auto;
  background-position:center center !important;
  background-size:contain !important;
  background-repeat: no-repeat !important;
  @media(max-width: 767px){
    width :175px;
    height:175px;
    margin:auto;
  }
}

ul.lightSlider[data-product="6"] .product_link.product_img{
  height:175px;
}

.product_main .product_link .product_img {
  visibility:hidden;
}
.product_main .quick_view{
  z-index: 3;
}

.sorting{
  margin-bottom:20px;
}

.footer_delivery {
  margin: 0 15px;
  border-top: 2px solid #adacac;
  padding-top: 7px;
  padding-bottom: 20px;
  img{
    max-width: 450px;
  }
}
.product_detail_title{
  cursor:pointer;
  .fa{
    float: right;
    margin-right: 10px;
    font-size: 15px;
    margin-top: 7px;
  }
}


.faq-main{
  padding: 0 !important;
  #shopify-section-page-faq{
    padding: 0;
    #faq_page{
      .top_logos{
        padding: 90px 0;
        .single_logo{
          width: 120px;
          text-align: center;
          @media(max-width: 767px){
            margin-bottom: 15px;
          }
          a{
            font-weight: bold;
            font-size: 20px;
            display: block;
            margin-top: 10px;

          }
        }
      }
      .page_template{
        padding: 0;
        .faq_header{
          background-color: #f4f4f4;
          height: 288px;
          position: relative;
          @media(max-width: 767px){
            height: 240px;
          }
          .head{
            position: absolute;
            margin-left: auto;
            margin-right: auto;
            bottom: 20px;
            width: 100%;
            height: auto;
            color: #000;
            font-family: "brandon-grotesque",sans-serif;
            font-style: normal;
            text-transform: uppercase;
            background: transparent;
            text-align: center;
            font-weight: 700px;
            font-size: 60px;
            p{
              margin-bottom: 0;
              line-height: 60px;
              font-weight: 700;
            }

            p:nth-of-type(1) {
              @media(max-width: 480px){
                font-size: 31.2px;
                line-height: 31.2px;
              }
            }
            p:nth-of-type(2) {
              @media(max-width: 480px){
                font-size: 37.2px;
                line-height: 37.2px;
              }
            }
          }
        }
        .speak-to-us{
          margin-top: 20px;
        }
        .qna{
          .qna_head{
            margin-top: 90px;
            @media(max-width: 767px){
              margin-top: 40px;
            }
            p{
              padding-left: 25%;
              font-weight: 300;
              font-size: 32px;
              text-transform: uppercase;
              color: #000;
              line-height: 45px;
              margin: 25px 0 -15px;
              @media(max-width: 1280px){
                padding-left: 20%;
              }
              @media(max-width: 480px){
                padding-left: 10%;
              }
            }
          }

          .faq_content{
            margin-top: 12px;
            background-color: #f4f4f4;
            border: 0;
            margin-bottom: 0;
            -webkit-box-shadow: none;
            box-shadow: none;
            padding-top: 15px;
            padding-bottom: 15px;
            .que{
              padding: 10px 15px;
              padding-left: 25%;
              border: 0;
              margin-top: 0;
              margin-bottom: 0;
              font-size: 18px;
              color: #000;
              font-weight: 600 !important;
              cursor: pointer;
              @media(max-width: 767px){
                font-size: 16.8px;
                line-height:16.8px;
              }
              @media(max-width: 1280px){
                padding-left: 20%;
              }
              @media(max-width: 480px){
                padding-left: 10%;
              }
            }
            .ans{
              display: none;
              padding-top: 0;
              padding-left: 25%;
              padding-bottom: 25px;
              border: 0;
              font-size: 18px;
              font-weight: 400;
              color: #000;
              @media(max-width: 1280px){
                padding-left: 20%;
              }
              @media(max-width: 480px){
                padding-left: 10%;
              }
              .content{
                p{
                  line-height: 26px;
                  font-size: 16px;
                  font-weight: 300;
                }
              }
            }
          }
        }

        .speak-to-us{
          p{
            margin-bottom: 0;
          }
          .help-sections-title{
            padding-left: 25%;
            font-weight: 300;
            font-size: 32px;
            text-transform: uppercase;
            color: #000;
            line-height: 45px;
            @media(max-width: 1280px){
              padding-left: 20%;
            }
            @media(max-width: 480px){
              padding-left: 10%;
              font-size: 20px;
            }
          } 
          .speak-to-us-desc{
            width: 100%;
            padding-top: 36px;
            padding-bottom: 36px;
            padding-left: 25%;
            background-color: #f4f4f4;
            @media(max-width: 1280px){
              padding-left: 20%;
            }
            @media(max-width: 480px){
              padding-left: 10%;
            }
            .title {
              font-size: 22px;
              color: #000;
              margin-bottom: 15px;
              line-height: 32px;
              @media(max-width: 767px){
                font-size: 16.8px;
                line-height: 32px;
              }
            }
            .info {
              p{
                font-weight: 400;
                font-size: 16px;
                line-height: 24px;
                color: #000;
                @media(max-width: 767px){
                  font-size: 16.8px;
                  line-height: 24px;
                }
                a{
                  color: #000;
                  border-bottom: 1px dotted #d8d8d8;
                }
              }
            }
            .speak-to-us-left-div {
              width: 400px;
              @media(max-width: 600px){
                width: 100%;
              }
            }
            .speak-to-us-div {
              float: left;
              display: inline-block;
            }
            .speak-to-us-right-div {
              width: 450px;
              @media(max-width: 600px){
                width: 100%;
              }
            }
            .top-div {
              height: 120px;
              @media(max-width: 767px){
                height: auto;
                padding-bottom: 20px;
              }
            }
            .bottom-div {
              height: auto;
              @media(max-width: 767px){
                height: auto;
                padding-bottom: 20px;
              }
            }

          } 
        }

      }
    }
  }
}


.input_form{
  margin-bottom: 12px;
  height:40px;
  border-radius: 33px;
  color: #080808;
  text-indent:5px;
  border: 1px solid #eee;
  background:#ffffff;
  @media(max-width:1024px){
    margin-bottom:12px;
  }
  @media(min-width:300){
    margin-bottom:12px;
  }
}

#register_page{
  margin-top: -15px;
  margin-bottom: -30px;
  background:#e84541;
  .full_area{
    background: #7a68cc;
  }
  .hat_img{
    @media(max-width:767px){
      .img{
        text-align:center;
        img{
          max-width:200px;
        }
      }
    }
  }


  @media(max-width:767px){
    margin-top:0;
    padding-top:15px;
  }
  .reg_content{
    color:#fff;
    padding:50px 0;
    #sign_head{
      color:#fff;
      font-size:25px;
      margin-bottom: 25px;
    }
    .fa-facebook{
      font-size:25px;
      margin-top: 8px;
    }
    .fa-twitter{
      font-size:25px;
      margin-top: 8px;
    }

    .btn_social{
      font-size:13px;
      height:45px;
      text-transform: none;
      color:#fff;
      border: 1px solid #5060a0;
      padding: 0px;
      width: 100%;
      margin-bottom: 15px; 
      padding-bottom:8px;
      height: 40px;
      span{
        margin-top: 11px;
        margin-left: 5px;
      }
      .fa,span{
        display: inline-block;
        vertical-align: top;
      }

    }
    .btn_done{
      height: auto;
      width: auto;
      text-transform: none;
      border-radius: 33px;
      color: #fff;
      padding: 15px 12px;
      min-width: 120px;
      font-size: 14px;
      margin-bottom: 15px;
      line-height: 1;
      background: #000;
      border: none ;
      &:hover{
        color: #ddd;
      }
    }
    .btn_tw{
      border-radius:33px;
      background:#467aff;
    }
    .btn_fb{
      border-radius:33px;
      background: #565bb3;
      color: #fff;
    }
    .signin{
      text-decoration:none;
    }
  }
}



