  /*       Checkbox button style */     
  .container-ckeck {
    display: block;
    position: relative;
    padding-left: 35px;
    cursor: pointer;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    line-height: 21px;
  }
  .container-ckeck input {
    position: absolute;
    opacity: 0;
    cursor: pointer;
    height: 0;
    width: 0;
  }
  .checkmark {
    position: absolute;
    top: 0;
    left: 0;
    height: 20px;
    width: 20px;
    background-color: #fff;
    border: 1px solid #49a99b;
  }
  .container-ckeck:hover input ~ .checkmark {
    background-color: #ccc;
  }
  .container-ckeck input:checked ~ .checkmark {
    background-color: #49a99b;
  }
  .checkmark:after {
    content: "";
    position: absolute;
    display: none;
  }
  .container-ckeck input:checked ~ .checkmark:after {
    display: block;
  }
  .container-ckeck .checkmark:after {
    left: 7px;
    top: 3px;
    width: 5px;
    height: 8px;
    border: solid white;
    border-width: 0 3px 3px 0;
    -webkit-transform: rotate(45deg);
    -ms-transform: rotate(45deg);
    transform: rotate(45deg);
  }      

