.owl-theme .owl-controls{
  width: 100%;
  height: 0px;
  opacity: 1;
  position: absolute;
  top: 40%;
  -webkit-transform: translateY(-50%);
  -ms-transform: translateY(-50%);
  transform: translateY(-50%);
  
  @media(max-width:767px){
    top: 36%;
  }
}
.owl-theme .owl-controls .owl-buttons div{
  width: 30px;
  height: 30px;
  line-height: 30px;
  border-radius: 0;
  background: transparent;
  
  @media(max-width:767px){
    width: 24px;
  height: 24px;
  line-height: 24px;
  }
}
.owl-prev{
  position: absolute;
  left: -3.7%;
  
