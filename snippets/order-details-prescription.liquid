      <div class="pimage {% if forloop.index == 1 %}selected{% endif%} skuak{{ line_item.sku }}" data-skuak= "skuak{{ line_item.sku }}" data-skuonly= "{{ line_item.sku }}">      
        <img class="{% if forloop.index == 1 %}active{% endif%}" src="{{ line_item | img_url: '355x' }}" alt=""/>
        <div class="title">{{ line_item.product.title }}</div>
       
        {% assign count = count | plus: 1 %}        
        <div class="item_id hide">{{ count }}</div>
        
        <div class="item_sku hide">{{ line_item.sku }}</div>
        <div class="item_type hide">{{ line_item.product.type }}</div>
        <span class="icon"></span>  
        <div class="usermessage"></div>
        <div class="sku{{ line_item.sku }}" style="margin-top: 7px; font-size: 16px; text-align: center; color:green;"></div>
      </div>
      {% endif %}     
    {% endfor %}
