$colorripp: #b4e0fe;
.ripple-box{
  background-color: transparent;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100%;  
  z-index: 0;
  position:relative;
}

.pulse1,
.pulse2,
.pulse3,
.pulse4 {
  width: 700px;
  height: 700px;
  -webkit-box-shadow:inset 0px 0px 40px 0px #b4e0fe;
  -moz-box-shadow:inset 0px 0px 40px 0px #b4e0fe;
  box-shadow:inset 0px 0px 40px 0px #b4e0fe;
  transition:box-shadow 2s;
  border-radius: 50%;
  z-index: -1;
  text-align: center;
  position: absolute;
  transform: scale(0.1, 0.1);
}
.pulse1 {
  animation: ring-1 4s 4.5s ease-out infinite;
}
.pulse2 {
  animation: ring-1 4s 3s ease-out infinite;
}
.pulse3 {
  animation: ring-1 4s 3.5s ease-out infinite;
}
.pulse4 {
  animation: ring-1 4s 4s ease-out infinite;
}
@keyframes ring-1 {
  0% {
    transform: scale(0.1, 0.1);
    opacity: 0
  }
  50% {
    opacity: 1
  }
  100% {
    transform: scale(1.2, 1.2);
    opacity: 0
  }
}
