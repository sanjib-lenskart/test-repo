/*==============================================================
Compare page start 
================================================================*/
.comparemain-lk{
  padding-left: 0!important;
  padding-right: 0!important;

  .compare-lk{
    padding-left: 0!important;
    padding-right: 0!important;
    margin-left: 0!important;
    margin-right: 0!important;
    padding:0;  
    background: url(//static2.lenskart.com/media/desktop/img/ditto_revamp_bg.svg) no-repeat center center;
    height:auto;
    width: 100%;
    background-size: cover;
    position:relative;

    @media(min-width:768px){
      height:calc(100vh - 60px);
    }

    .box-comtainer{    
      min-width: 320px;
      background: #fff;
      padding: 15px 10px; 

      @media(min-width:768px){
        min-width: 640px;
        padding: 15px 0;
        position: absolute;
        top: 50%;
        left: 50%;
        -webkit-transform: translate(-50%, -50%);
        -moz-transform: translate(-50%, -50%);
        -ms-transform: translate(-50%, -50%);
        -o-transform: translate(-50%, -50%);
        transform: translate(-50%, -50%);

      }

      @media(max-width:767px){
        padding: 0px; 
      }

      .text-wrapper{
        padding:30px;

        @media(max-width:767px){
          background-image: linear-gradient(128deg,#f6be30,#07b2b6);
          color: #fff;
          height: 100vw;
        }


        @media(max-width:767px){
          padding:20px 15px 40px;
        }

        .text{
          margin-bottom:5px; 
          letter-spacing: 0.5px; 
          text-align: center;


          @media(max-width:767px){
            font-size:14px;
            margin-bottom:10px; 
          }
          p{
            display:inline-flex;
            font-size: 16px;

            .distext{
              max-width: 285px;
              display: block; 
              line-height: 21px;
              letter-spacing: 0.8px;
              @media(max-width:767px){
                max-width: 280px;
                font-weight: 600;
              }
            }
          }
          .notifi{
            position: relative;
            display: inline-block;
            z-index: 100;
            width: 2rem;
            height: 2rem;
            margin-right: 12px;
            margin-right: .75rem;
            margin-bottom: 0;
            border: 1px solid #fff;
            line-height: 2rem;
            text-align: center;
            color: #fff;
            background-color: transparent;
            border-radius: 50%;

            @media(min-width:768px){
              border: 1px solid #000;
              color: #000;
            }

          }
        }

        h1{
          text-transform:uppercase;
          margin-bottom: 20px;

          @media(max-width:767px){
            margin-bottom: 10px;
            font-size: 26px;
            color: #fff;
          }
        }
      }  
    }

    button {
      background-color: #2fbba4;
      min-width: 200px;
      margin: 30px auto 0;
      padding: 10px;
      display: block;
      text-align: center;
      color: #fff;
      font-size: 16px;
      border: 1px solid transparent;
      font-weight: 700;
      line-height: 1.42857;
      border-radius: 4px;
      -webkit-user-select: none;
      -moz-user-select: none;
      -ms-user-select: none;
      user-select: none;

      @media(max-width:767px){
        margin: 10px auto 20px;
        background-color: #fff;
        color: #2fbba4;
      }
    }
    .img-wrapper{

      @media(max-width:767px){
        padding:0;
      }

      img{
        width:100%;      
      }
    }
  }
}

